<elements>
    <info>
        <name>Dreadlord Specialization for Warlord</name>
        <description></description>
        <update version="0.0.1">
            <file name="warlord-dreadlord.xml" url="https://raw.githubusercontent.com/ShadowLinks/KibblesTasty-elements/refs/heads/main/Compendium/warlord-dreadlord.xml" />
